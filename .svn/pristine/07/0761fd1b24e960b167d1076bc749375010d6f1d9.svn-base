.tabDisplay{
    text-align: center;
    list-style: none;
    margin: 0px;
    padding-left: 0;
    height: 50px;
    right: 0;
    position: absolute;
    z-index: 1000;
    margin-right: 6px;
}
.tab {
    float: left;
    padding: 10px;
    border: 1px solid #ffffff;
    border-right: 0px;
    border-top: 0px;
    margin-top: 1px;
    cursor: pointer;
    min-width: 100px;
    background-color: #188FBE;
    color: #ffffff;
}

.tabDisplay > .tab:active{
    border-bottom: none;
    color:#48ABD2;
}

.tabActive{
    border-bottom: none;
    color:#48ABD2;
    background:none;
}

.tabActive:first-child{
    border-left:none;
}

.tab:last-child{
    border-radius: 0px 22px 0px 0px;
}

.tab:first-child{
    border: none;
}

