
<div id="wrapper" ng-controller="ProfileController as profile">
    <div class="row"></div>
    <header-section></header-section>
    <div class="container" style="padding-top:80px; position:relative;">
        <div class="row">
            <div class="col-md-10 col-md-offset-1">
                <div class="panel panel-default" style="min-height:500px">
                    <form name="UserForm" novalidate>
                        <div class="form_style">
                            <ul>
                                 <span style="margin-top:-2px;float:right; border:1px solid #000000;background:#FAA9A3;color:#000000;">
                                   <span>* Mandatory</span>
                                 </span>
                                <li class="marB0px">
                                    <h5></span></h5>
                                </li>
                                <li class="dsp_block">
                                    <div class="pull-left check-box-style wd60p" ng-show="_userInfo.IsAuthenticate==true || profile._info.IsIDAvailable==true">
                                        <span class="pull-left wd20p" style="margin-top: 1.3%">User Type :</span>
                                        <input type="radio" name="EZType" ng-model="profile._info.IDTypeID" id="Individual" class="check-box pull-left emptyBox"
                                               ng-class="UserForm.$submitted && UserForm.EZType.$error.required ? 'mandatoryTextBox' : ''" value="1" required/>
                                        <label for="Individual" class="pull-left under_label">Individual</label>
                                        <input type="radio" name="EZType" ng-model="profile._info.IDTypeID" id="Business" class="check-box pull-left emptyBox"
                                               ng-class="UserForm.$submitted && UserForm.EZType.$error.required ? 'mandatoryText' : ''" value="2" required>
                                        <label for="Business" class="pull-left under_label">Business</label>
                                        <input type="radio" name="EZType" ng-model="profile._info.IDTypeID" id="Venue" class="check-box pull-left emptyBox"
                                               ng-class="UserForm.$submitted && UserForm.EZType.$error.required ? 'mandatoryText' : ''" value="3">
                                        <label for="Venue" class="pull-left under_label">Public Place</label>
                                    </div>
                                    <div class="clearfix"></div>
                                </li>
                                <li class="dsp_block">
                                    <div class="pull-left wd50p">

                                        <input type="text" style='text-transform:uppercase' class="form-control wd30p pull-left emptyBox wd50p" ng-pattern="/^[a-zA-Z0-9]*$/"
                                               name="EZEID" ng-model="profile._info.EZEID" placeholder="Your preferred EZEID"
                                               maxlength="15" ng-class="UserForm.$submitted  UserForm.EZEID.$error.required ? 'mandatoryTextBox' : ''"
                                               required/>

                                        <input type="button" name="btnCheckID" class="btn btn-primary pull-left wd40p check" style="margin-left: 1%;"
                                               ng-show="_userInfo.IsAuthenticate==true" ng-click="profile.CheckisIDAvailable()"
                                               ng-disabled="profile._info.EZEID==undefined || profile._info.IsIDAvailable==true" value="Check Availability"/>
                                        <div class="clearfix"></div>

                                            <span ng-show="UserForm.EZEID.$touched" class="font12px dsp_block padT10px">
                                                <span style="color:red" ng-if="UserForm.EZEID.$touched && (profile._info.IsIDAvailable==false || profile._info.IsAuthenticate==false)" class="font14px">
                                                    Your preferred EZEID is not available</span>
                                                <span ng-show="UserForm.EZEID.$touched && (profile._info.IsIDAvailable==true || profile._info.IsAuthenticate==true)" class="green font14px">
                                                    Your preferred EZEID is available</span>
                                            </span>
                                    </div>
                                    <div class="pull-left wd25p">
                                        <input type="password" name="PWD" class="form-control wd90p emptyBox pass1_marLeft" ng-model="profile._info.Password"
                                               ng-class="UserForm.$submitted && UserForm.PWD.$error.required ? 'mandatoryText' : ''" placeholder="Password"
                                               maxlength="15" required/>
                                    </div>
                                    <div class="pull-left wd25p">
                                        <input type="password" name="CPWD" class="form-control wd90p emptyBox pass2_marLeft" ng-model="profile._info.CPassword"
                                               placeholder="Reenter Password" maxlength="15"
                                               ng-class="UserForm.$submitted && UserForm.CPWD.$error.required ? 'mandatoryText' : ''" required/>
                                    </div>
                                    <div class="clearfix marB5px"></div>

                                    <div class="pull-left wd90p">
                                        <select type="text" ng-init="profile._info.NameTitleID=1" ng-model="profile._info.NameTitleID"
                                                ng-options="title.TitleID as title.Title for title in profile.Titles"
                                                class="form-control pull-left wd_px1"></select>
                                        <div class="pull-left wd40p">
                                            <input type="text" name="FName" ng-model="profile._info.FirstName" class="form-control emptyBox wd90p"
                                                   placeholder="First Name"
                                                   ng-class="UserForm.$submitted && UserForm.Add1.$error.required ? 'mandatoryText' : ''" required/>
                                        </div>
                                        <div class="pull-left wd40p">
                                            <input type="text" name="LName" ng-model="profile._info.LastName" class="form-control emptyBox lastName_mar wd90p"
                                                   placeholder="Middle & Last Name" ng-class="UserForm.$submitted && UserForm.Add2.$error.required ? 'mandatoryText' : ''" required/>
                                        </div>
                                        <div class="clearfix marB5px"></div>
                                    </div>
                                    <div class="clearfix marB5px"></div>
                                 <li>
                                     <div class="select-map">

                                         <div>
                                             <img id="mapCloc" style="padding-top:8px;cursor:pointer;" ng-click="profile.getMyLocation()" src="images/myloc.png" />
                                         </div>
                                         <div style="background-color:white;height:50px;">
                                             <input id="txtSearch" class="form-control pull-left marL5px" type="text" ng-model="profile._mapSearchText" placeholder="Search Location" autocomplete="off" style=" z-index: 0; top:0px;margin-top:10px; width: 400px;">
                                             <button class="btn btn-primary pull-left marL10px marT10px" ng-click="profile.SearchOnMap(profile._mapSearchText)" ng-disabled="profile._mapSearchText==undefined" type="button">Find</button>
                                         </div>
                                         <div id="map" style="width: 100%;height: 278px;position: relative;overflow: hidden;transform: translateZ(0px) translateZ(0px);background-color: rgb(229, 227, 223);border-radius: 6px;">
                                         </div>
                                     </div>
                                 </li>
                                <li class="dsp_block">
                                    <div class="pull-left wd50p">
                                        <button name="pick_loc" type="button" ng-class="UserForm.$submitted && UserForm.LatLong.$error.required ? 'mandatoryText' : ''" class="btn btn-info btn-xs" ng-click="profile.isMapInitialized()" required>Pick your Address & location from map
                                        </button>
                                        <!--<label>
                                            <a href="#" name="pick_loc" ng-click="profile.isMapInitialized()"
                                               class="pick-location" ng-class="UserForm.$submitted && UserForm.LatLong.$error.required ? 'mandatoryText' : ''"
                                               required>Pick your Address & location from map</a>
                                        </label>-->
                                    </div>

                                    <div class="pull-right wd48p" style="position: absolute;margin-left: 59%;">
                                        <img src="images/user_200.png" width="200" style="margin-top: -5%;"/>

                                        <div style="margin-top: -2%; margin-left: 10%;">
                                            <button style="width:127px!important;" class="btn btn-primary">Attach Picture</button>
                                        </div>
                                        <div style="margin-left: 9%;margin-top: 1%;">
                                            <img src="images/defaultSmallicon.png"> <button style="padding-left:2px;width:88px!important;" class="btn btn-primary">Attach Logo</button>
                                        </div>

                                        <input id="PinCheck" style="margin-left:-5px;  margin-top: 20px;" type="checkbox" class="pull-left wd20p check-box" ng-model="profile._info.PinCheck"/>&nbsp;
                                        <label for="PinCheck"  style="margin-left:-5px; margin-top: 22px;" class="pull-left wd25p">Protect your ID</label>
                                        <input type="text" class="form-control" style="width: 12%;" maxlength="3" placeholder="PIN">
                                       <!-- <div style="margin-left: 10%;margin-top: 1%;">
                                            <input type="text" class="form-control" style="width: 36%;margin-left: 0%;width:127px!important;" ng-model="profile._info.CompanyName" placeholder="Company Name">
                                        </div>

                                        <div style="margin-left: 10%;margin-top: 1%;">
                                            <input type="text" class="form-control" style="width: 36%;margin-left: 0%;width:127px!important;" ng-model="profile._info.JobTitle" placeholder="Title">
                                        </div>-->


                                    </div>
                                    <div class="clearfix marB5px"></div>
                                    <div class="pull-left wd28p">
                                        <input type="text" name="Add1" ng-model="profile._info.AddressLine1" class="form-control emptyBox wd90p"
                                               placeholder="#No, StreetName.."
                                               ng-class="UserForm.$submitted && UserForm.Add1.$error.required ? 'mandatoryText' : ''" required/>
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input type="text" name="Add2" ng-model="profile._info.AddressLine2" class="form-control emptyBox wd90p"
                                               placeholder="Block, Sector etc.." ng-class="UserForm.$submitted && UserForm.Add2.$error.required ? 'mandatoryText' : ''" required/>
                                    </div>
                                    <div class="clearfix marB5px"></div>


                                    <div class="pull-left wd28p">
                                        <select class="form-control emptyBox wd90p" name="countries" ng-model="profile._info.CountryID"
                                                ng-change="profile.getStates(profile._info.CountryID)"
                                                ng-options="country.CountryID as country.CountryName for country in profile.countries"
                                                ng-class="UserForm.$submitted && UserForm.countries.$error.required ? 'mandatoryText' : ''" required></select>
                                    </div>
                                    <div class="pull-left wd28p">
                                        <select class="form-control emptyBox wd90p" name="states" ng-model="profile._info.StateID"
                                                ng-change="profile.getCities(profile._info.StateID)"
                                                ng-options="state.StateID as state.StateName for state in profile.states"
                                                ng-class="UserForm.$submitted && UserForm.states.$error.required ? 'mandatoryText' : ''" required></select>
                                    </div>
                                    <div class="clearfix marB5px"></div>


                                    <div class="pull-left wd28p">
                                        <input class="form-control emptyBox wd90p" name="cities" type="text" ng-model="profile._info.CityTitle"
                                               typeahead="city.CityName as city.CityName for city in profile.cities" placeholder="City"
                                               ng-class="UserForm.$submitted && UserForm.cities.$error.required ? 'mandatoryText' : ''" required />
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input type="text" name="postalCode" class="form-control emptyBox wd90p" ng-model="profile._info.PostalCode"
                                               placeholder="Postal Code" numbers-only="numbers-only">
                                    </div>
                                    <div class="clearfix marB5px"></div>



                                    <div class="pull-left wd28p">
                                        <input style="padding-left:0px;" type="text" class="form-control wd20p pull-left" value="+91" disabled>
                                        <input type="tel" name="MobileNumber" class="form-control wd70p pull-left emptyBox"
                                               ng-model="profile._info.MobileNumber" placeholder="Mobile Number" maxlength="10" numbers-only="numbers-only"
                                               ng-class="UserForm.$submitted && UserForm.MobileNumber.$error.required ? 'mandatoryText' : ''" required/>
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input style="padding-left:0px;" type="text" class="form-control wd20p pull-left" value="+91" disabled>
                                        <input type="tel" class="form-control wd70p pull-left"
                                               ng-model="profile._info.PhoneNumber" placeholder="Phone Number" maxlength="10" numbers-only="numbers-only">
                                    </div>
                                    <div class="clearfix marB5px"></div>


                                    <div class="pull-left wd28p">
                                        <input type="email" name="email" class="form-control emptyBox wd90p"
                                               ng-model="profile._info.EMailID" placeholder="Email ID" ng-pattern="/^[a-z]+[a-z0-9._]+@[a-z]+\.[a-z.]{2,5}$/"
                                               ng-class="UserForm.$submitted && UserForm.email.$error.required ? 'mandatoryText' : ''"/>
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input type="text" name="website"
                                               ng-model="profile._info.Website" class="form-control wd90p" placeholder="Website">
                                    </div>
                                    <div class="clearfix marB5px"></div>
                                    <div class="pull-left wd28p">
                                        <input type="text" class="form-control wd90p" ng-model="profile._info.LaptopSLNO" placeholder="Laptop Model & SL. Number">
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input type="text" class="form-control wd90p" ng-model="profile._info.VehicleNumber" placeholder="Vehicle Model & Number">
                                    </div>
                                    <div class="clearfix marB5px"></div>
                                    <div class="pull-left wd28p">
                                        <input type="text" class="form-control wd90p" ng-model="profile._info.CompanyName" placeholder="Company Name">
                                    </div>
                                    <div class="pull-left wd28p">
                                        <input type="text" class="form-control wd90p" ng-model="profile._info.JobTitle" placeholder="Title">


                                    </div>
                                    <div class="pull-right wd28p">
                                       <!-- <input id="PinCheck" style="margin-left:-5px;" type="checkbox" class="pull-left wd20p check-box" ng-model="profile._info.PinCheck"/>&nbsp;
                                        <label for="PinCheck"  style="margin-left:-5px;" class="pull-left wd80p">Protect your ID by a PIN</label>
                                        <input type="text" class="form-control wd25p" maxlength="3" placeholder="PIN">-->
                                        <!--<input id="PinCheck" style="margin-left:-5px;" type="checkbox" class="pull-left wd20p check-box" ng-model="profile._info.PinCheck"/>&nbsp;
                                        <label for="PinCheck" class="pull-left wd80p">Protect your ID by a PIN</label>-->
                                    </div>
                                    <div class="pull-right wd28p">
                                        <!--<input type="text" class="form-control wd25p" maxlength="3" placeholder="PIN">-->
                                    </div>
                                    <div class="clearfix marB5px"></div>

                                    <div class="clearfix marB5px"></div>
                                    <!--<div class="pull-left wd28p">
                                        <input id="PinCheck" style="margin-left:-5px;" type="checkbox" class="pull-left wd20p check-box" ng-model="profile._info.PinCheck"/>&nbsp;
                                        <label for="PinCheck" class="pull-left wd80p">Protect your ID by a PIN</label>
                                    </div>-->
                                    <!--<div class="pull-left wd28p">
                                        <input type="text" class="form-control wd90p" ng-model="" placeholder="Enter Pin Number">
                                    </div>-->

                                 <div>
                                     <input id="terms" type="checkbox" style="margin-left:-5px; width: 6%;" class="pull-left check-box"/>
                                     <span class="termsMargin">I agree to the <a href="#">terms and conditions</a> </span>
                                 </div>
                                 <div class="clearfix "></div>
                                <li class="marT10px">

                                    <input type="submit" ng-hide="_userInfo.IsAuthenticate" ng-click="profile.savePrimaryRegistration(UserForm)"
                                           class="btn-primary btn" value="Generate EZEID " />
                                    <input type="submit" ng-show="_userInfo.IsAuthenticate" ng-click="profile.savePrimaryRegistration(UserForm)"
                                           class="btn-primary btn" value="Update EZEID" />
                                     <input type="submit" ng-click="profile.savePrimaryRegistration(UserForm)"
                                            class="btn-primary btn wd10p" value="Cancel" />
                                </li>
                                </li>

                            </ul>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
